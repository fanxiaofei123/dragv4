/*搜索框*/
.search-common{
  width: 242px;
}
#addWorkDetails .form-group textarea{
  height: 100px;
}
.treeDeomBox{
  position: relative;
  /*width: 100%;*/
  height: 200px;
  border: 1px solid #dfe3e9;
  border-radius: 5px;
}
.dataList .table-hover > #flowWorkTbody2 .gradeX .actions{
  position: absolute;
  width: 90px;
  left: -100px;
  color: #fff;
  display: none;

}
.line{
  color: #dfe3e9;
  padding: 0 18px;
  font-size: 18px;
}
.page-bar div span:first-child{
  font-size: 16px;
  /*color: #64848e;*/
  letter-spacing: 1px;
}

.page-bar div i{
  font-size: 20px;
  margin: 0 5px;
}
.page-bar div .creat_model{
  position: relative;
  bottom: -6px;
  text-align: left;
}
.page-bar div .creat_model>span span{
  position: absolute;
  color: #fff;
}
.page-bar div .creat_model>span{
  position: absolute;
  font-size: 12px;
  color: #fff;
  left: 20px;
}
.haveCheckBox{
  background-color: #abbcc4!important;
  pointer-events: none;
}
.haveCheckIcon{
  color: #abbcc4!important;
  pointer-events: none;
}