@charset "UTF-8";
.vh{
    visibility: hidden;
}
.index{
    position: relative;
}
.text{
    color: #0f9b7e;
    font-size: 16px;
}
.index .table{
    border-radius: 5px!important;
    background: #1386a4;
    border-color: #4896a9;
}
.index .table thead{
    background: #0f6d86;
}
.index .table thead tr th{
    padding-left: 30px;
    font-weight: normal;
    color: #032935;
}
.index .table-hover tbody tr{
    height: 50px;
    background: #1386a4;
}
.index .table > tbody > tr > td, .table > tfoot > tr > td, .table > thead > tr > td{
    line-height: 50px;
    padding-left: 30px;
    color: #fff6ec;
}
.index .table > tbody td:nth-child(1){
    color: #073443;
    font-weight: 500;
    color: #fff6ec;
}
.index .table thead,
.index .table > tbody td:nth-child(2),
.index .table > tbody td:nth-child(5)
{
    color: #64848e;
    color: #fff6ec;
}
.index .table > tbody td:nth-child(4){
    color: #1bbc9b;
    color: #fff6ec;
}
.index .table > tbody td:nth-child(3){
    color: #09485d;
    color: #fff6ec;
}
.index .table > tbody > tr > td span{
    display: inline-block;
    width: 52px;
    color: #09ffb1;
}
.index .table > tbody > tr > td i{
    /*border: 1px solid #1bbc9b;*/
    /*border-radius: 10px;*/
}
.index .table > tbody > tr > td .glyphicon{
    font-weight: normal!important;
}
.index>.table-hover>tbody>tr:hover{
    background: #d6fef6;
    color: #4896a9!important;
    transform: scale(1.02);
    transition-duration: 0.5s;
    -webkit-transform: scale(1.02);
}
.index>.table-hover>tbody>tr:hover td{
    color: #4896a9!important;
    border-right: none;
}
.model .text,caption.text{
    margin-top: 10px;
    padding: 20px 0;
    overflow: hidden;
}
.model .modelList{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    min-width: 1280px;
}
.creat{
    position: relative;
    width: 15%;
    border-radius: 5px!important;
    text-align: center;
    background: #1bbc9b;
    overflow: hidden;
    height: 245px;
}
.creat:first-child{
    /*margin-left: 0;*/
}
.creat:nth-child(6){
    margin-right: 0;
}
.creat>p{
    position: absolute;
    bottom: 10%;
    width: 100%;
    text-align: center;
    color: white;
}
.creat .des{
    position: absolute;
    top: 0;
    width: 100%;
    height: 100%;
    background: #1bbc9b;
    color: #073443;
}
.creat .des p{
    margin: 0;
}
.creat .des>p{
    padding: 4% 0;
    vertical-align: middle;
    background: #149284;
    color: #fff;
    font-size: 16px;
}
.creat .des .des_width{
    width: 100%;
    padding: 0 8%;
}
.creat .des .des_width p{
    color: #073443;
    padding: 10px 0;
}
.creat .des .des_width p:last-child{
    height: 20%;
    overflow: hidden;
    text-align: left;
    color: #fff;
    line-height: 2;
    font-size: 12px;
}
.creat .des img{
    margin: 0 auto;
}
.creat .see{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(19, 145, 179, 0.7);
    opacity: 0;
}
.creat .see a{
    margin: 8px auto;
    padding: 5% 0;
    display: block;
    width: 67%;
    text-align: center;
    color: #fff;
    border: 1px solid #fff;
    border-radius: 5px!important;
}
.creat .see a:first-child{
    margin-top: 32%;
}
.index>.table-hover>tbody>tr,.creat .see,.creat .see a{
    cursor: default;
    transition: all .3s linear;
    -moz-transition: all .3s linear; /* Firefox 4 */
    -webkit-transition: all .3s linear; /* Safari 和 Chrome */
    -o-transition: all .3s linear; /* Opera */
}
.creat .see:hover{
    opacity: 1;
}
.creat .see a:hover{
    background: rgba(255, 255, 255, 0.6);
    color: #0f7a96;
    border: 1px solid rgba(255, 255, 255, 0.6);
}
.creat .des h6{
    position: absolute;
    left: 7%;
    bottom: 0;
    text-align: left;
    color: #fff;
    margin-left: 1%;
}
/*新加样式覆盖*/
.index .table > tbody > tr > td{
    border-top: 1px solid rgba(205, 221, 227, 0.31);
}
.table > thead > tr > th{
    border-bottom: none;
}
