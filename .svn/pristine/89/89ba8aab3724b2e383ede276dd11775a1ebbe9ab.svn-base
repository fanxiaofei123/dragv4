/*2D球指定位置效果*/
.circle{
    position: absolute;
    left: -12px;
    top: 80px;
    width: 12px;
    height: 12px;
    border-radius: 12px;
    background: #fff;
}
.circle-choose{
    -webkit-transform: scale(35.0);
    opacity: 0;
    transition: all .6s linear;
    -moz-transition: all .6s linear; /* Firefox 4 */
    -webkit-transition: all .6s linear; /* Safari 和 Chrome */
    -o-transition: all .6s linear; /* Opera */
}
/*end*/
/*轮播指示条*/
.slide{
    position: relative;
    z-index: 2;
    bottom: 100px;
    margin-top: 30%;
}
/*轮播启动按钮*/
.slideSpinner{
    position: relative;
    float: right;
    top: 30px;
    right: 18px;
    width: 46px;
    height: 46px;
}
.centerSpinner{
    margin: 17px;
    width: 12px;
    height: 12px;
    border-radius: 100%;
    background: rgba(9, 255, 178, 0.6);
}
.spinner {
    position: absolute;
    top: 0;
    left: 0;
    margin: 3px;
    width: 40px;
    height: 40px;
    background-color: rgba(9, 255, 178, 0.8);
    border-radius: 100%;
    -webkit-animation: scaleout 2.0s infinite ease-in-out;
    animation: scaleout 2.0s infinite ease-in-out;
}

@-webkit-keyframes scaleout {
    0% { -webkit-transform: scale(0.0);background-color: rgba(9, 255, 178, 0.8); }
    100% {
        -webkit-transform: scale(1.0);
        opacity: 0;
        background-color: #09ffb2;
    }
}

@keyframes scaleout {
    0% {
        transform: scale(0.0);
        -webkit-transform: scale(0.0);
    } 100% {
          transform: scale(1.0);
          -webkit-transform: scale(1.0);
          opacity: 0;
      }
}
/*end*/
/*轮播启动按钮效果*/
.slideSpinner:before{
    position: absolute;
    bottom: 23px;
    right: 37px;
    content: "";
    border-right: 1px solid #fff;
    width: 0;
    /* height: 25px; */
     height: 0;
    display: inline-block;
    transform: rotate(135deg);
}
.slideSpinner:after{
    position: absolute;
    top: -18px;
    right: 46px;
    display: inline-block;
    width: 0;
    height: 21px;
    content: "";
    border-bottom: 2px solid #fff;
    text-align: center;
    letter-spacing: 2px;
    color: #fff;
}
.slideSpinner:hover:before{
     height: 25px;
 }
.slideSpinner:hover:after{
    transition-delay: 1s;
    -moz-transition-delay: 1s; /* Firefox 4 */
    -webkit-transition-delay: 1s; /* Safari 和 Chrome */
    -o-transition-delay: 1s; /* Opera */
    width: 56px;
    content: "start";
}
/*end*/
.slideBox{
    height: 450px;
}
.slideBox div.hd{
    position: relative;
    left: 30px;
    width: 30px;
    height: 100%;
}
.slideBox div ul{
    position: absolute;
    bottom: 0;
    padding: 0;
    width: 30px;
}
.slideBox div ul li{
    margin-top: 30px;
    list-style: none;
    width: 15px;
    height: 15px;
    background: #1dbca3;
    border-radius: 15px;
}
.slideBox,.slideBox div ul li,
.slideSpinner:hover:after,.slideSpinner:hover:before{
    -webkit-transition: all .5s linear;
    -moz-transition: all .5s linear;
    -ms-transition: all .5s linear;
    -o-transition: all .5s linear;
    transition: all .5s linear;
}
.slideBox div ul li:hover{
    height: 80px;
}
.slideBox div ul li.on{
    height: 80px;
}
.slideBox div.hd , .slideBox div.bd{
    float: left;
}
.slideBox div.bd{
    width: 95%;
}
/*转动球体*/
.IcosahedronGeometry{
    position: absolute;
    z-index: 1;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}
/*信息*/
#spritesName{
    position: absolute;
    z-index: 1;
    color: #ffffff;
    font-size: 16px;
}