package org.com.drag.model;

import java.io.Serializable;

/**
 * Created by Guzhendong on 2017/10/19 0019.
 */
public class TemplateCategory  implements Serializable {
    private static final long serialVersionUID = 109908970273152890L;

    public Integer getUserid() {
        return userid;
    }

    public void setUserid(Integer userid) {
        this.userid = userid;
    }

    private Integer userid;
    private Integer templateCategoryId;

    /**
     * 模板分类名称
     */
    private String templateCategoryName;

    /**
     * 父节点id
     */
    private Integer templateCategoryPid;

    /**
     * 是否是叶子节点，是：1，否：0
     */
    private Integer templateCategoryIsParent;

    public Integer getTemplateCategoryId() {
        return templateCategoryId;
    }

    public void setTemplateCategoryId(Integer templateCategoryId) {
        this.templateCategoryId = templateCategoryId;
    }

    /**
     * @return 模板分类名称
     */
    public String getTemplateCategoryName() {
        return templateCategoryName;
    }

    /**
     * @param templateCategoryName
     *            模板分类名称
     */
    public void setTemplateCategoryName(String templateCategoryName) {
        this.templateCategoryName = templateCategoryName;
    }

    /**
     * @return 父节点id
     */
    public Integer getTemplateCategoryPid() {
        return templateCategoryPid;
    }

    /**
     * @param templateCategoryPid
     *            父节点id
     */
    public void setTemplateCategoryPid(Integer templateCategoryPid) {
        this.templateCategoryPid = templateCategoryPid;
    }

    public Integer getTemplateCategoryIsParent() {
        return templateCategoryIsParent;
    }

    public void setTemplateCategoryIsParent(Integer templateCategoryIsParent) {
        this.templateCategoryIsParent = templateCategoryIsParent;
    }

}

