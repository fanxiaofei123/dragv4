		    .bodyBox,.release_info{
		    	display: none;
		    }
			.bodyBox{
				width: 100%;
				height: auto;
				position: absolute;
				top: 0;
				left: 0;
				filter:alpha(opacity=50);
				opacity: 0.5;
				background-color: #aaa;
				z-index: 20;
			}
			
			.release_info{
				background-color: white;

				position: absolute;
				height: 500px;
				width: 400px;
				top: 50%;
				left: 50%;
				margin-left: -250px;
				margin-top: -100px;
				border-radius:4px ;
				z-index: 21;

				
			}
			.tell{
				width: 100%;
				background-color: #33cccc;
				padding-top: 10px;
				padding-bottom: 10px;
				color: white;
				border-top-left-radius: 5px;
				border-top-right-radius: 5px;
	
            }
            .info{
				position: absolute;
				max-height: 400px;
				overflow: auto;
				width: 100%;
            }
			.yes,.no{
				position: absolute;
				bottom: 0;
				right: 10px;
				display: inline-block;
				width: 100px;
				background-color: #33cccc;
				padding: 5px 10px;
				color: white;
				border-radius: 5px;
				margin: 15px 0;
			}
            .yes{
				right:120px;
            }
			.tree{
				border: none;
			}
			input[type=checkbox]{
				display: inline-block;
				margin: 4px 4px 4px 0;
			}