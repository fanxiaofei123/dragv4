.font-color{
    color: #1abc9c;
}
.dn{
    display: none;
}
.menu_title{
    width: 100%;
    height: 60px;
    padding: 20px;
    background: #fff;
}
.filter{
    position: relative;
    float: left;
    padding: 0 28px;
    border-left: 1px solid #cbcfd2;
}
.filter ul{
    position: absolute;
    z-index: 2;
    top: 22px;
    right: 28px;
    padding: 4px 5px;
    background: #fff;
    box-shadow: 0 0 5px #dfe3e5;
    border-radius: 5px!important;
    border: 1px solid #d0e0e4;
}
.filter ul li{
    list-style: none;
    height: 36px;
    line-height: 36px;
    cursor: default;
}
.filter ul li i{
    color: #fff;
}
.filter ul li span{
    display: inline-block;
    min-width: 88px;
    text-align: center;
}
.filter ul li.filter-active,
.filter ul li.filter-active i{
    color: #1abc9c;
}
.filter ul li:hover,
.filter ul li:hover i{
    background: #1abc9c;
    color: #fff;
}
/*模板库样式*/
.creat{
    position: relative;
    padding: 10px 0;
    margin: 10px 9px;
    width: 250px;
    height: 320px;
    border-radius: 5px!important;
    text-align: center;
    background: #fff;
    overflow: hidden;
    box-shadow: 0 0 5px #dfe3e5;
}
.creat:nth-child(6){
    margin-right: 0;
}
.creat .des{
    margin-top: 10px;
    width: 100%;
    height: 100%;
    text-align: left;
    color: #073443;

}
.creat .des p{
    margin: 0;
    line-height: 1.6;
}
.creat .des>p{
    padding: 4% 0;
    vertical-align: middle;
    background: #149284;
    color: #fff;
    font-size: 16px;
}
.creat .des .des_width{
    width: 100%;
    padding: 0 8%;
}
.creat .des .des_width p:nth-child(1){
    color: #073443;
}
.creat .des .des_width p:nth-child(2){
    color: #85989e;
    padding: 8px 0;
    font-size: 12px;
}
.creat .des .des_width p:last-child{
    height: 20%;
    overflow: hidden;
    text-align: left;
    color: #999999;
    font-size: 12px;
}
.creat .des img{
    margin: 0 auto;
}
.creat .see{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(26, 188, 156, 0.78);
    opacity: 0;
}
.creat .see a{
    margin: 16px auto;
    padding: 5% 0;
    display: block;
    width: 67%;
    text-align: center;
    color: #fff;
    border: 1px solid #fff;
    border-radius: 5px!important;
}
.creat .see a:first-child{
    margin-top: 40%;
}
.index>.table-hover>tbody>tr,.creat .see,.creat .see a{
    cursor: default;
    transition: all .3s linear;
    -moz-transition: all .3s linear; /* Firefox 4 */
    -webkit-transition: all .3s linear; /* Safari 和 Chrome */
    -o-transition: all .3s linear; /* Opera */
}
.creat .see:hover{
    opacity: 1;
}
.creat .see a:hover{
    background: rgba(255, 255, 255, 0.6);
    color: #1abc9c;
    border: 1px solid rgba(255, 255, 255, 0.6);
}
/*end*/
/*flex*/
.templateBase{
    display: flex;
    flex-flow: row wrap;
    justify-content: flex-start;
    align-content: flex-start;
    align-items: flex-start;
}
.templateBase .creat{
    /*flex-grow: 2;*/
}
/*page*/
.pagination {
    box-shadow: 0 0 5px #dfe3e5;
}
.pagination > .active > a, .pagination > .active > a:focus, .pagination > .active > a:hover, .pagination > .active > span, .pagination > .active > span:focus, .pagination > .active > span:hover {
    background: #1bbc9b;
    border-color: #1bbc9b;
}
/*弹框*/
.treeDeomBox {
    position: relative;
    height: 200px;
    border: 1px solid #dfe3e9;
    border-radius: 5px;
}